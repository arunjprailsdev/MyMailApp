<p id="notice"><%= notice %></p>

<h1>Banks</h1>

<table>
  <thead>
    <tr>
      <th>Postdate</th>
      <th>Bankname</th>
      <th>Postname</th>
      <th>Qualification</th>
      <th>Lastdate</th>
      <th>Description</th>
      <th>Qualification desc</th>
      <th>Totalpost</th>
      <th>Agelimit</th>
      <th>Selectionprocess</th>
      <th>Applicationfees</th>
      <th>Applylink</th>
      <th>Lastdate</th>
      <th>Lastdateonlineapplication</th>
      <th>Callletter</th>
      <th>Callletterlink</th>
      <th>Dateofexam</th>
      <th>Howtoapply</th>
      <th>Officialnotification</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @banks.each do |bank| %>
      <tr>
        <td><%= bank.PostDate %></td>
        <td><%= bank.BankName %></td>
        <td><%= bank.PostName %></td>
        <td><%= bank.Qualification %></td>
        <td><%= bank.LastDate %></td>
        <td><%= bank.Description %></td>
        <td><%= bank.Qualification_desc %></td>
        <td><%= bank.TotalPost %></td>
        <td><%= bank.AgeLimit %></td>
        <td><%= bank.SelectionProcess %></td>
        <td><%= bank.ApplicationFees %></td>
        <td><%= bank.ApplyLink %></td>
        <td><%= bank.LastDate %></td>
        <td><%= bank.LastDateOnlineApplication %></td>
        <td><%= bank.CallLetter %></td>
        <td><%= bank.CallLetterLink %></td>
        <td><%= bank.DateofExam %></td>
        <td><%= bank.HowtoApply %></td>
        <td><%= bank.OfficialNotification %></td>
        <td><%= link_to 'Show', bank %></td>
        <td><%= link_to 'Edit', edit_bank_path(bank) %></td>
        <td><%= link_to 'Destroy', bank, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Bank', new_bank_path %>
